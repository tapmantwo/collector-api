{
  "compilerOptions": {
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["dom", "es5", "es6"],
    "inlineSourceMap": true,
    "allowJs": false,
    "strictFunctionTypes": false,
    "strictNullChecks": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["api/**/*.ts"],
  "exclude": ["node_modules"]
  // "ts-node": {
  //   "files": true,

  // }
}
